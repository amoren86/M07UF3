<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
	version="3.0">
    <persistence-unit name="SocialPersistenceUnit" transaction-type="JTA">
        <jta-data-source>jdbc/social</jta-data-source>
        <class>cat.institutmarianao.jpa.model.User</class>
        <properties>
            <property name="eclipselink.logging.level" value="FINEST"/>
        </properties>
    </persistence-unit>
</persistence>
